
MEMORY
{
    ram : ORIGIN = 0x8000, LENGTH = 0x2000000
}

SECTIONS
{
    .text : { *(.text*) } > ram
    .bss : { *(.bss*) } > ram
    .data : { *(.data*) } > ram
    .rodata : { *(.rodata*) } > ram
    .symdata : { *(.symdata*) } > ram
    __exidx_start = .;
    .ARM.exidx : {*(.ARM.exidx* .gnu.linkonce.armexidx.*) } > ram
    __exidx_end = .;
    
}

