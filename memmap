
MEMORY
{
    ram : ORIGIN = 0x8000, LENGTH = 0x200000
}

SECTIONS
{
    .text : { *(.text*) } > ram
    .bss : { *(.bss*) } > ram
    .data : { *(.data*) } > ram
    .rodata : { *(.rodata*) } > ram    
    __exidx_start = .;
    .ARM.exidx : {*(.ARM.exidx* .gnu.linkonce.armexidx.*) } > ram
    __exidx_end = .;
    .symdata : { *(.symdata*) } > ram
    __kernel_end = .;    
}

